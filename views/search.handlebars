<div class="container">
    <form class="m-2 search-form">
        <div class="row m-1">
            <div class="form-group col-12 col-lg-11">
                <input type="text" class="form-control search-field" placeholder="Search...">
            </div>
            <div class="form-group col-12 col-lg-1 d-flex justify-content-center">
                <button type="submit" class="btn btn-primary">Submit</button>
            </div>
        </div>
    </form>
    <div class="row d-flex" id="search-results">
        {{> searchresults results}}
    </div>
</div>
